#include "Sodium.xcconfig"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Private" "${PODS_ROOT}/Headers/Private/Sodium" "${PODS_ROOT}/Headers/Public"
LIBRARY_SEARCH_PATHS = ${SODIUM_LIBRARY_SEARCH_PATHS}
OTHER_LDFLAGS = ${SODIUM_OTHER_LDFLAGS}
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_ROOT = ${SRCROOT}
SKIP_INSTALL = YES